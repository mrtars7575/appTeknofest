<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".othersFragment">

    <ImageView
        android:id="@+id/noteIv"
        android:layout_width="117dp"
        android:layout_height="61dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="81dp"
        app:layout_constraintEnd_toStartOf="@+id/alarmIv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/note"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/alarmIv"
        android:layout_width="97dp"
        android:layout_height="73dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/noteIv"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/alarm"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/calculateIv"
        android:layout_width="138dp"
        android:layout_height="63dp"
        android:layout_marginTop="120dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.42"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/noteIv"
        app:srcCompat="@drawable/calculate"
        tools:src="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>